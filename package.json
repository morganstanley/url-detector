{
  "name": "@morgan-stanley/url-detector",
  "version": "0.1.0-beta.3",

  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "A command-line tool that scans source code and text files for URLs, detecting all discovered URLs",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest --config jest.config.js",
    "test:coverage": "jest --config jest.config.js --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf dist",
    "pkg": "npm run pkg:all",
    "pkg:all": "npm run pkg:linux && npm run pkg:macos && npm run pkg:win",
    "pkg:linux": "node scripts/generate-pkg-config.js linux > pkg-linux.json && npx @yao-pkg/pkg dist/cli.js --config pkg-linux.json --output url-detector-linux && rimraf pkg-linux.json",
    "pkg:macos": "node scripts/generate-pkg-config.js macos > pkg-macos.json && npx @yao-pkg/pkg dist/cli.js --config pkg-macos.json --output url-detector-macos && rimraf pkg-macos.json",
    "pkg:win": "node scripts/generate-pkg-config.js win > pkg-win.json && npx @yao-pkg/pkg dist/cli.js --config pkg-win.json --output url-detector-win.exe && rimraf pkg-win.json"
  },
  "bin": {
    "url-detector": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "scanner"
  ],
  "homepage": "https://github.com/morganstanley/url-detector",
  "author": "Morgan Stanley",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morganstanley/url-detector.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@tree-sitter-grammars/tree-sitter-kotlin": "^1.1.0",
    "@tree-sitter-grammars/tree-sitter-toml": "^0.7.0",
    "@tree-sitter-grammars/tree-sitter-xml": "^0.7.0",
    "@tree-sitter-grammars/tree-sitter-yaml": "^0.7.1",
    "cli-table3": "^0.6.0",
    "commander": "^14.0.0",
    "fast-glob": "^3.3.3",
    "minimatch": "^10.0.3",
    "p-limit": "^3.1.0",
    "tree-sitter": "^0.22.4",
    "tree-sitter-bash": "^0.21.0",
